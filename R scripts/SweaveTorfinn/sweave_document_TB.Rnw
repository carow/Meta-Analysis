text(4*ci.lb, rma.RE$k+2, "Study",    pos = 4, cex = .68, font = 2)
text(3*ci.ub, rma.RE$k+2, "Weights   Effect sizes [95% CI]",  pos = 2, cex = .68, font = 2)
par (mar = c(5,4,4,2))
@

