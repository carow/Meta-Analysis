                            "pQ" = as.vector (sens.RE$Qp),
                            "I^2" = as.vector (sens.RE$I2),
                            "ESchgabs"=as.vector (abs(sens.RE$estimate - rma.RE$b)),                          
                           "Weight" = as.vector(weight.RE) )
                            
